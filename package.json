{
    "author": "Markus Per√∂bner <markus.peroebner@gmail.com>",
    "name": "osm-read",
    "description": "an openstreetmap XML and PBF data parser",
    "tags": [
        "osm",
        "openstreetmap"
    ],
    "version": "0.5.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/marook/osm-read.git"
    },
    "bugs": {
        "url": "https://github.com/marook/osm-read/issues"
    },
    "main": "lib/main.js",
    "browser": {
        "protobufjs": "./node_modules/protobufjs/ProtoBuf.noparse.min.js"
    },
    "scripts": {
        "test": "mocha test/*.js",
        "buildProto": "node_modules/protobufjs/bin/proto2js lib/proto/fileformat.proto -commonjs > lib/proto/fileformat.js && node_modules/protobufjs/bin/proto2js lib/proto/osmformat.proto -commonjs > lib/proto/osmformat.js",
        "browserify": "browserify ./lib/pbfParser.js --bare -x bytebuffer -x long -r ./node_modules/protobufjs/node_modules/bytebuffer/ByteBuffer.min.js:bytebuffer -r ./node_modules/protobufjs/node_modules/bytebuffer/node_modules/long/dist/Long.min.js:long -s=pbfParser -o osm-read-pbf.js"
    },
    "engines": [
        "node >= 0.10.0"
    ],
    "dependencies": {
        "node-xml": "1.0.2",
        "protobufjs": "2.0.5"
    },
    "optionalDependencies": {
        "zlibjs": "0.1.7"
    },
    "devDependencies": {
        "mocha": "1.19.0",
        "should": "3.3.1"
    },
    "licenses": [
        {
            "type": "LGPLv3",
            "url": "http://www.gnu.org/licenses/lgpl-3.0-standalone.html"
        }
    ]
}
